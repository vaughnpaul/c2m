msgstr "URL e imazhit per ikonen."

#: socialize-this-php4-enviroment.php:315
#: socialize-this-php5-enviroment.php:315
msgid "The alternative text for the icon."
msgstr "Teksti alternativ per ikonen."

#: socialize-this-php4-enviroment.php:319
#: socialize-this-php5-enviroment.php:319
msgid "The TinyURL of your post. Generated on Publish, if tiny is not present permalink will be used. (URL Encoded)"
msgstr "TinyURL-ja e artikullit tuaj. Krijuar gjate publikimit, nese nuk eshte prezente do te perdoret permalink. (URL Encoded)"

#: socialize-this-php4-enviroment.php:323
#: socialize-this-php5-enviroment.php:323
msgid "Your RSS URL"
msgstr "URL-ja juaj e RSS"

#: socialize-this-php4-enviroment.php:329
#: socialize-this-php5-enviroment.php:329
msgid "Credits"
msgstr "Kreditet"

#: socialize-this-php4-enviroment.php:330
#: socialize-this-php5-enviroment.php:330
msgid "Coder"
msgstr "Programuesi"

#: socialize-this-php4-enviroment.php:331
#: socialize-this-php5-enviroment.php:331
msgid "Testing Server"
msgstr "Server Testimi"

#: socialize-this-php4-enviroment.php:332
#: socialize-this-php5-enviroment.php:332
msgid "Icons"
msgstr "Ikonat"

#: socialize-this-php4-enviroment.php:345
#: socialize-this-php5-enviroment.php:346
msgid "Title"
msgstr "Titulli"

